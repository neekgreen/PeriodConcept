@model PeriodConcept.Web.Areas.Database.Models.Searchable.IndexViewModel

<div class="row">
    <div class="col-md-10">
        @Html.Partial("SearchableFormPartial", Model)
    </div>
    <div class="col-md-2">
        @Html.Partial("ItemCountPerPagePartial", Model)
    </div>
</div>

<nav>
    @Html.Partial("PaginableSummaryPartial", Model.Periods)
</nav>

<div id="paginable" data-page-number="@Model.PageNumber">
    @foreach (var period in Model.Periods)
    {
        @Html.Partial("PeriodPartial", period)
    }
</div>

<nav>
    @Html.Partial("PaginableSummaryPartial", Model.Periods)
    @Html.Partial("PaginablePartial", Model.Periods)
</nav>